generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Vehicle {
  id             String  @id @default(uuid())
  make           String
  model          String
  year           Int
  color          String
  type           String
  licensePlate   String  @unique
  imageUrl       String
  steamId        String  // identificador do dono
  customizations Json
  performance    Json
}
